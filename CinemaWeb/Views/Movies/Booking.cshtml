@model BookingModel

<nav aria-label="breadcrumb">
    <ol class="breadcrumb bg-dark">
        <li class="breadcrumb-item active text-warning" aria-current="page">Current bookings</li>
    </ol>
</nav>

<div class="card mb-3" style="max-width: 1200px;">
    <div class="row no-gutters">
        <div class="col-md-4">
            <svg class="bd-placeholder-img card-img-top" width="100%" height="180" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: Image cap"><title>Placeholder</title><rect width="100%" height="100%" fill="#868e96"></rect><text x="50%" y="50%" fill="#dee2e6" dy=".3em"></text></svg>
        </div>
        <div class="col-md-8">
            <div class="card-body">
                <blockquote class="blockquote text-right">
                    <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-x" viewBox="0 0 16 16">
                        <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z" />
                    </svg>
                </blockquote>
                <a asp-controller="Movies" asp-action="Category" class="text-warning">Title</a>
                <p class="card-text"><small class="text-muted">15:00</small></p>
            </div>
        </div>
    </div>
</div>